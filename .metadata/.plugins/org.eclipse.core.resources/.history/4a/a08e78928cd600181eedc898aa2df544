package com.java.basics.fileutilities;

import java.io.File;

public class FileUtilities {

	public static void main(String[] args) {
		File file = null;
		File file1 = new File("")
		file = new File("/Users/Pradhi/workspace-java-neon3/008JavaIO");
		
		String[] path = file.list();
		for(String p:path){
			System.out.println(p);
		}

	}

}
