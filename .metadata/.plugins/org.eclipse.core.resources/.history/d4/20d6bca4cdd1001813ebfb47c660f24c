


public class HelloWorld {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int result = Integer.parseInt(args[0]) + Integer.parseInt(args[1]);
		System.out.println("Add two arguments: " + result);
	}
	
	public static void main(String args) {
		// TODO Auto-generated method stub
		int result = Integer.parseInt(args);
		System.out.println("Only one argument: " + result);
	}
	
	public static void main() {
		// TODO Auto-generated method stub
		System.out.println("There are no rules; Cheers!!!! ");
	}


}
