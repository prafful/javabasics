package com.java.classes;

public class Friend {
	public int age;
	public int location;
	public int numberOfYears;
	String name;
	
	
	public Friend(int age, int location, int numberOfYears, String name) {
		super();
		this.age = age;
		this.location = location;
		this.numberOfYears = numberOfYears;
		this.name = name;
	}

	public Friend() {
		super();
	}
	
	
	
	
}
