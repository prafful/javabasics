package com.java.loops;

public class JavaLoops {
	public static void main(String[] args) {
		int i=5;
		
		/*while (i<=10) {
			System.out.println(++i);
			//1-10, 2-11, 1-11
		}*/
		/*do {
			System.out.println(++i);
		} while (i<=5);*/
		for (int j = 0; j < i; j++) {
			System.out.println(j);
		}
		
		System.out.println("java Loops");
		
	}
}
